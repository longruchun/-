package com.shaodq.chapter2.d20180724;

public class jiujiu {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        for(int i=1;i<=9;i++){
        	for(int j=1;j<=i;j++){
        		System.out.print(i+" * "+j+" = "+(i*j)+"  ");
        	}
        	System.out.println();
        }
	}

}
